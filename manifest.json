{
  "manifest_version": 3,
  "name": "Tab Switcher Bear",
  "version": "1.34",
  "description": "Switch tabs with a cute preview using one shortcutâ€”like Cmd+Tab for your browser.",
  "key": "",
  "icons": {
    "16": "icons/icon-16x16.png",
    "32": "icons/icon-32x32.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "action": {
    "default_icon": "icons/icon-48x48.png",
    "default_title": "Tab Switcher",
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "commands": {
    "open_switcher": {
      "suggested_key": {
        "default": "Alt+Q",
        "mac": "Alt+Q"
      },
      "description": "Open switcher popup / Next"
    },
    "switch-to-left-tab": {
      "suggested_key": {
        "default": "Alt+Left",
        "mac": "Alt+Left"
      },
      "description": "Switch to the tab to the left"
    },
    "switch-to-right-tab": {
      "suggested_key": {
        "default": "Alt+Right",
        "mac": "Alt+Right"
      },
      "description": "Switch to the tab to the right"
    }
  }
}
